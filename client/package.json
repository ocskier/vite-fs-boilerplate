{
  "name": "vite-fs-boilerplate",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "cy-comp-coverage": "cypress run -b chrome --component",
    "cy-comp-coverage-show": "cypress run -b chrome --component --headed",
    "cy-open": "cypress open",
    "dev": "vite",
    "build": "tsc && vite build",
    "build-storybook": "storybook build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 8",
    "prettier:fix": "prettier -w .",
    "prettier:check": "prettier -c .",
    "pre-commit": "lint-staged",
    "prepare-husky": "chmod +x ./prepare-husky.sh && ./prepare-husky.sh",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "test": "env-cmd -f .env.dev.local vitest run -u --coverage --silent",
    "test-coverage": "VITE_ENV=dev vitest run -u --coverage --silent",
    "test-watch": "env-cmd -f .env.dev.local vitest -u"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.2.6",
    "@storybook/test": "^7.6.17",
    "@tanstack/react-query": "^5.51.5",
    "@testing-library/jest-dom": "^6.1.2",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@vitejs/plugin-basic-ssl": "^1.1.0",
    "@vitejs/plugin-react": "^4.0.4",
    "@vitest/coverage-v8": "^0.34.6",
    "axios": "^1.7.2",
    "axios-mock-adapter": "^1.22.0",
    "graphql": "^16.9.0",
    "graphql-request": "^7.1.0",
    "jsdom": "^22.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-oidc-context": "^3.1.0",
    "react-redux": "^9.1.2",
    "react-router-dom": "^6.26.1",
    "vite": "^4.4.9",
    "vitest": "^0.34.3"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.13.1",
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@originjs/vite-plugin-commonjs": "^1.0.3",
    "@storybook/addon-essentials": "^7.6.17",
    "@storybook/addon-interactions": "^7.6.17",
    "@storybook/addon-links": "^7.6.17",
    "@storybook/addon-onboarding": "^1.0.11",
    "@storybook/blocks": "^7.6.17",
    "@storybook/react": "^7.6.17",
    "@storybook/react-vite": "^7.6.17",
    "@types/node": "^20.5.7",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.5.0",
    "@typescript-eslint/parser": "^6.5.0",
    "cypress": "^12.17.4",
    "eslint": "^8.48.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.8.0",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "prettier": "^3.0.3",
    "storybook": "^7.6.17",
    "typescript": "^5.0.2",
    "vite-plugin-istanbul": "^6.0.2"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx,js}": [
      "npm run prettier:fix",
      "npm run lint"
    ]
  }
}
